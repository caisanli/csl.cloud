{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "corejs": 3,
                "targets": [
                    "last 1 version",
                    "> 1%",
                    "IE 10"
                ]
            }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        "react-hot-loader/babel",
        "@babel/plugin-transform-runtime"
    ]
}